		<Replace Language="en_US" Tag="LOC_LEADER_AZUR_LANE_YUUDACHI_NAME" Text="Yuudachi"/>
		<Replace Language="en_US" Tag="LOC_LEADER_AZUR_LANE_SHIGURE_NAME" Text="Shigure"/>
